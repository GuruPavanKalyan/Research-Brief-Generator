{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üåê Debug FastAPI API (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.api.main:app",
        "--reload",
        "--port",
        "8000"
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "üß™ Debug CLI Script",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/cli/main.py",
      "args": [],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal"
    }
  ]
}
